<!DOCTYPE html>
<!-- Request ID: <%= $c->req->request_id %> -->
<html>
  <head>
    <title>Page not found</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <style>
      a img { border: 0 }
      body { background-color: #caecf6 }
      #noraptor {
        left: 0%;
        position: fixed;
        top: 60%;
      }
      #notfound {
        background: url(<%= url_for '/mojo/notfound.png' %>);
        width: 306px;
        height: 62px;
        margin-left: -153px;
        margin-top: -31px;
        position: absolute;
        top: 50%;
        left: 50%;
      }
    </style>
  </head>
  <body>
    %= link_to url_for->base => begin
      %= image '/mojo/noraptor.png', alt => 'Bye!', id => 'noraptor'
    % end
    <div id="notfound"></div>
  </body>
</html>
